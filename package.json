{
  "name": "ember-audio",
  "version": "1.0.0-beta.1",
  "description": "An Ember addon that makes working with the Web Audio API super EZ",
  "keywords": [
    "ember-addon",
    "audio",
    "sound",
    "music",
    "synthesis",
    "soundfont",
    "sampling",
    "media",
    "mp3",
    "wav",
    "ogg",
    "web",
    "audio",
    "api"
  ],
  "repository": "https://github.com/sethbrasile/ember-audio",
  "license": "MIT",
  "author": "Seth Brasile <seth.brasile@gmail.com>",
  "directories": {
    "doc": "tests/dummy/public/docs",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "deploy": "ember build --environment production && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages"
  },
  "dependencies": {
    "ember-auto-import": "^1.2.20",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-fetch": "^6.4.0",
    "stereo-panner-node": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@ember/jquery": "^0.5.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.7.0",
    "@embroider/test-setup": "^1.6.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^3.4.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.1",
    "ember-cli": "~4.4.0",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-stylus": "^1.0.7",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-yuidoc": "^0.8.8",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-font-awesome": "^4.0.0-rc.4",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-prism": "^0.4.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.8.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.72.1",
    "yuidoc-ember-cli-theme": "^1.0.4"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "greenkeeper": {
    "ignore": [
      "bootstrap"
    ]
  }
}
